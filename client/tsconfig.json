{
	"compilerOptions": {
		"baseUrl": "./",
		"outDir": "./build/",
		"noImplicitAny": true,
		"sourceMap": true,
		"module": "es6",
		"target": "es5",
		"moduleResolution": "node",
		"jsx": "react",
		"allowJs": true,
		"rootDir": "src"
	},
	"paths": {
		"@/*": ["src/*"]
	},
	"include": [
		"src/**/*"
	],
	"exclude":[
		"node_modules"
	]
}